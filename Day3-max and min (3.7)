#include <stdio.h>
int main()
{
int n,arr[50],min,max,i;
printf("Enter the number:");
scanf("%d",&n);
for(i=0;i<n;i++)
{
printf("Enter Element %d:",i);
scanf("%d",&arr[i]);
}
min = arr[0];
max = arr[0];


for(i=1;i<n;i++)
{
if(arr[i]>max)
{
max=arr[i];
}
if(arr[i]<min)
{
min=arr[i];
} 
}
printf("Max:%d\n",max);
printf("Min:%d\n",min);
return 0;
}
OUTPUT:
Enter the number:3
Enter Element 0:1
Enter Element 1:5
Enter Element 2:2
Max:5
Min:1
