#include<stdio.h>
int main()
{
int i, j, k;
int arr[3][4][2] = {
{ {2, 4}, {7, 8}, {3, 4}, {5, 6} },
{ {7, 6}, {3, 4}, {5, 3}, {2, 3} },
{ {8, 9}, {7, 2}, {3, 4}, {5, 1} }
};
for(i=0; i<3; i++)
{
for(j=0; j<4; j++)
{
    for(k=0; k<2; k++)
printf("arr[%d][%d][%d] = %d ", i, j, k, arr[i][j][k]);
printf("\n");
}
printf("\n");
}
return 0;
}

OUTPUT:
arr[0][0][0] = 2 arr[0][0][1] = 4 
arr[0][1][0] = 7 arr[0][1][1] = 8 
arr[0][2][0] = 3 arr[0][2][1] = 4 
arr[0][3][0] = 5 arr[0][3][1] = 6 

arr[1][0][0] = 7 arr[1][0][1] = 6 
arr[1][1][0] = 3 arr[1][1][1] = 4 
arr[1][2][0] = 5 arr[1][2][1] = 3 
arr[1][3][0] = 2 arr[1][3][1] = 3 

arr[2][0][0] = 8 arr[2][0][1] = 9 
arr[2][1][0] = 7 arr[2][1][1] = 2 
arr[2][2][0] = 3 arr[2][2][1] = 4 
arr[2][3][0] = 5 arr[2][3][1] = 1 
